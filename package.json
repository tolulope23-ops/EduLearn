    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.3",
    "mysql2": "^3.17.1",
    "nodemailer": "^8.0.1",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.5",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@playwright/test": "^1.58.2",
    "@types/node": "^25.2.3",
