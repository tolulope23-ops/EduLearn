{
  "name": "edulearn",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "nodemon src/index.js",
    "dev": "node src/index.js",
    "lint": "eslint src ",
    "lint:fix": "eslint src --fix",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "migrate:create": "npx sequelize-cli migration:generate",
    "migrate:up": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "migrate:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "seed:create": "npx sequelize-cli seed:generate",
    "seed": "npx sequelize-cli db:seed",
    "seed:undo": "npx sequelize-cli db:seed:undo --seed",
    "migrate:prod": "NODE_ENV=production npx sequelize-cli db:migrate",
    "seed:prod": "NODE_ENV=production npx sequelize-cli db:seed --seed"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cors": "^2.8.6",
